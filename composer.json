        "symfony/routing": "^4.4 || ^5.3 || ^6.0",
        "symfony/security-acl": "^3.1",
        "symfony/security-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/security-core": "^4.4.24 || ^5.3 || ^6.0",
        "symfony/security-csrf": "^4.4 || ^5.3 || ^6.0",
        "symfony/string": "^5.3 || ^6.0",
        "symfony/translation": "^4.4 || ^5.3 || ^6.0",
        "symfony/translation-contracts": "^1.1.10 || ^2.3 || ^3.0",
        "symfony/twig-bridge": "^4.4 || ^5.3 || ^6.0",
        "symfony/twig-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/validator": "^4.4 || ^5.3 || ^6.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12.1 || ^3.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.7",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16",
        "psalm/plugin-symfony": "^3.0",
        "psr/event-dispatcher": "^1.0",
        "sonata-project/intl-bundle": "^2.4",
        "symfony/browser-kit": "^4.4 || ^5.3 || ^6.0",
        "symfony/css-selector": "^4.4 || ^5.3 || ^6.0",
        "symfony/filesystem": "^4.4 || ^5.3 || ^6.0",
        "symfony/maker-bundle": "^1.25",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/yaml": "^4.4 || ^5.3 || ^6.0",
        "vimeo/psalm": "^4.9.2"
    },
    "conflict": {
        "doctrine/doctrine-bundle": ">=3"
    },
    "suggest": {
        "twig/extra-bundle": "Auto configures the Twig Intl extension"
    },
    "autoload": {
        "psr-4": {
            "Sonata\\AdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\AdminBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
