{
    "name": "sonata-project/admin-bundle",
    "description": "The missing Symfony Admin Generator",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "admin",
        "admin-generator",
        "admin-lte",
        "bundle",
        "sonata",
        "symfony"
    ],
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataAdminBundle/contributors"
        }
    ],
    "homepage": "https://docs.sonata-project.org/projects/SonataAdminBundle",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "doctrine/collections": "^1.6",
        "doctrine/common": "^3.0",
        "doctrine/inflector": "^2.0",
        "doctrine/persistence": "^2.0",
        "knplabs/knp-menu": "^3.1",
        "knplabs/knp-menu-bundle": "^3.0",
        "psr/container": "^1.0 || ^2.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "sonata-project/block-bundle": "^4.7",
        "sonata-project/doctrine-extensions": "^1.8",
        "sonata-project/exporter": "^2.11",
        "sonata-project/form-extensions": "^1.7.1",
        "sonata-project/twig-extensions": "^1.4.1",
        "symfony/asset": "^4.4 || ^5.3 || ^6.0",
        "symfony/config": "^4.4 || ^5.3 || ^6.0",
        "symfony/console": "^4.4 || ^5.3 || ^6.0",
        "symfony/dependency-injection": "^4.4.8 || ^5.3 || ^6.0",
        "symfony/doctrine-bridge": "^4.4 || ^5.3 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.3 || ^6.0",
        "symfony/event-dispatcher-contracts": "^1.1 || ^2.0 || ^3.0",
        "symfony/expression-language": "^4.4 || ^5.3 || ^6.0",
        "symfony/form": "^4.4.12 || ^5.3 || ^6.0",
        "symfony/framework-bundle": "^4.4.6 || ^5.3 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.3 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.3 || ^6.0",
        "symfony/options-resolver": "^4.4 || ^5.3 || ^6.0",
